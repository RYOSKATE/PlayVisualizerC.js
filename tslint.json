{
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "max-line-length": false,
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "import-name": false,
    "no-else-after-return": false,
    "no-increment-decrement": false,
    "variable-name": false,
    "prefer-template": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules",
      "src/**/*.d.ts",
      "src/node/Uni*.ts",
      "src/**/*Parser.ts",
      "src/**/*Lexer.ts",
      "src/**/*Visitor.ts"
    ]
  }
}